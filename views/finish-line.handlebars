<section>
    <h2>Finish Line</h2>
    {{!-- {{>category-post posts}} --}}

    {{#if posts.length}}
        <ol class="post-list">
            {{#each posts as |post|}}
                {{#if (check_if_equal category_id 7)}}
                    <li>
                        {{> post-info post}}
                    </li>
                {{/if}}
            {{/each}}
        </ol>
    {{/if}}

</section>
