<section>
    <h2>Self-Care</h2>

    {{!-- posts is an array of objects I would like to pass through the partial category-posts --}}
    {{!-- {{>category-post posts}} --}}
    
    {{#if posts.length}}
        <ol class="post-list">
            {{#each posts as |post|}}
                {{#if (check_if_equal category_name "self-care")}}
                    <li>
                        {{> post-info post}}
                    </li>
                {{/if}}
            {{/each}}
        </ol>
    {{/if}}
</section>